Public Class Eusuario

    Private Nombre As String
    Private Contraseña As String
    Private Roll As String

    Public Property _Nombre
        Get
            Return Nombre
        End Get
        Set(value)
            Nombre = value
        End Set
    End Property
    Public Property _Contraseña
        Get
            Return Contraseña
        End Get
        Set(value)
            Contraseña = value
        End Set
    End Property
    Public Property _Roll
        Get
            Return Roll

        End Get
        Set(value)
            Roll = value
        End Set
    End Property



End Class
